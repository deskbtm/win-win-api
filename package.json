{
	"name": "win32-ffi",
	"version": "0.0.6-alpha.4",
	"description": "js/ts win32 api base on ffi",
	"author": "Nawbc <deskbtm@outlook.com>",
	"homepage": "https://github.com/deskbtm/win32-ffi/",
	"license": "MIT",
	"main": "dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"lib/**",
		"dist/**"
	],
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"keywords": [
		"windows",
		"ffi",
		"electron",
		"node",
		"win32",
		"system"
	],
	"engines": {
		"node": ">=14"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/deskbtm/win32-ffi.git"
	},
	"scripts": {
		"build": "rimraf ./dist && tsc -p tsconfig.json",
		"start": "rimraf ./dist && tsc -w -p tsconfig.json",
		"gen": "node ./scripts/win_cpp_fns_to_ts_generator.js",
		"doc": "typedoc ./lib --readme ./README.md",
		"lint": "eslint lib  --fix --ext \".ts\" ",
		"test": "jest"
	},
	"bugs": {
		"url": "https://github.com/deskbtm/win32-ffi/issues"
	},
	"devDependencies": {
		"@types/ffi-napi": "^4.0.5",
		"@types/jest": "^27.4.1",
		"@types/ref-napi": "^3.0.4",
		"@types/ref-union-di": "^1.0.3",
		"@typescript-eslint/eslint-plugin": "^4.26.1",
		"@typescript-eslint/parser": "^4.26.1",
		"cheerio": "1.0.0-rc.10",
		"eslint": "^7.28.0",
		"eslint-plugin-import": "^2.23.4",
		"jest": "^27.5.1",
		"node-gyp": "^9.0.0",
		"npm-run-all": "^4.1.5",
		"request": "^2.88.2",
		"request-promise": "^4.2.6",
		"rimraf": "^3.0.2",
		"ts-jest": "^27.1.4",
		"typedoc": "^0.22.13",
		"typescript": "^4.3.2"
	},
	"dependencies": {
		"ffi-napi": "^4.0.3",
		"ref-napi": "^3.0.3",
		"ref-struct-di": "^1.1.1",
		"ref-union-di": "^1.0.1"
	}
}
